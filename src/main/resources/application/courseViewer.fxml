<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<Group xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CourseController">
   <children>
      <Pane fx:id="OriginalPane" prefHeight="768.0" prefWidth="1024.0">
         <children>
            <Button fx:id="addButton" layoutX="430.0" layoutY="672.0" mnemonicParsing="false" onAction="#CreateClick" prefHeight="53.0" prefWidth="139.0" snapToPixel="false" text="Add Course" />
            <Button fx:id="logoutButton" layoutX="18.0" layoutY="731.0" mnemonicParsing="false" onAction="#logoutButton" text="Logout" />
            <Label fx:id="warningLabel" layoutX="462.0" layoutY="27.0" text="Too many courses!" textFill="RED" visible="false" />
            <AnchorPane layoutX="18.0" layoutY="30.0" prefHeight="632.0" prefWidth="995.0">
               <children>
                  <TabPane fx:id="Table" prefHeight="633.0" prefWidth="995.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                      <Tab text="Untitled Tab 1">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                        </content>
                      </Tab>
                      <Tab text="Untitled Tab 2">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                        </content>
                      </Tab>
                    </tabs>
                  </TabPane>
               </children>
            </AnchorPane>
            <Button fx:id="delButton" layoutX="578.0" layoutY="672.0" mnemonicParsing="false" onAction="#CreateClick" prefHeight="53.0" prefWidth="139.0" snapToPixel="false" text="Delete Course" />
            <Button fx:id="modButton" layoutX="284.0" layoutY="672.0" mnemonicParsing="false" onAction="#CreateClick" prefHeight="53.0" prefWidth="139.0" snapToPixel="false" text="Modify Course" />
         </children>
      </Pane>
   </children>
</Group>
